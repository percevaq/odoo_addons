<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_partner_stock_form" model="ir.ui.view">
        <field name="name">res.partner.stock.property.form.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="stock.view_partner_stock_form"/>
        <field name="arch" type="xml">
            <group name="container_row_stock" position="inside">
                <group name="incoterms" string="incoterm">
                    <field name="property_sale_incoterm"/>
                    <field name="property_purchase_incoterm"/>
                </group>
            </group>
        </field>
    </record>
</odoo>
