<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_hs_code" model="ir.ui.view" >
        <field name="name">product.template.form.hs_code</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="delivery.product_template_hs_code"/>
        <field name="arch" type="xml">
            <field name="hs_code" position="replace">
                <field name="hs_code_id">
                    <form string="Hs Code">
                        <field name="code"/>
                        <field name="description"/>
                    </form>
                    <tree string="Hs Codes">
                        <field name="code"/>
                        <field name="description"/>
                    </tree>
                </field>
            </field>
        </field>
    </record>
</odoo>
